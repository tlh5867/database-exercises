USE employees;

-- Update your query for 'Irena', 'Vidya', or 'Maya' to use OR instead of IN â€” 709 rows.
SELECT *
FROM employees
WHERE first_name ='Irena'
 OR first_name='Vidya'
  OR first_name='Maya'
  AND gender= '%';


-- Employees whose last name starts with 'E'
SELECT *
FROM employees
WHERE last_name LIKE 'E%'
 OR last_name LIKE '%E';


 SELECT *
FROM employees
WHERE last_name LIKE 'E%'
 AND last_name LIKE '%E';

-- Employees hired in the 90s
SELECT *
FROM employees
WHERE hire_date BETWEEN '1990-01-01' AND '1999-12-31'
AND birth_date LIKE '%-12-25';

-- Employees born on Christmas
SELECT  *
FROM employees
WHERE birth_date like '%-12-25';

-- Employees with a 'q' in their last name
SELECT last_name
FROM employees
WHERE last_name like '%q%'
and last_name not like '%qu%';

SELECT hire_date, first_name, last_name
FROM employees
Group BY hire_date,first_name, last_name;